package com.test;

public class ObjectDemo extends Object{

	public static void main(String[] args) {
        A obj = new A();
        
        System.out.println(obj);
	}

}


class A extends Object
{
	int age = 10;
	String name= "abc";
	
	 public String toString() {
	        return getClass().getName() + "@" + Integer.toHexString(hashCode());
	    }
}